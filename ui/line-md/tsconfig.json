{
	"compilerOptions": {
		"rootDir": "src",
		"outDir": "lib",
		"target": "ESNext",
		"module": "ESNext",
		"declaration": false,
		"sourceMap": false,
		"strict": true,
		"types": ["svelte"],
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true
	}
}
