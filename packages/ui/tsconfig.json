{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"declaration": false,
		"sourceMap": false,
		"strict": true,
		"types": ["svelte"],
		"moduleResolution": "node",
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true
	}
}
